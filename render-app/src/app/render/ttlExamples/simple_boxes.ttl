Object:Method_Shape a occ:Shape .

#hasGeometry
Object:mainObject omg:hasGeometry Object:Box1,
        Object:Box2. 

#ComplexGeometryDescription 
Object:Box1 omg:hasComplexGeometryDescription Object:Box1_Desc .         
Object:Box2 omg:hasComplexGeometryDescription Object:Box2_Desc . 
Object:Point1 omg:hasComplexGeometryDescription Object:Point1_Desc .  



Object:Box1_Desc a occ:BRepPrimAPI_MakeBox ;
    oop:hasMethod Object:Method_Shape;
    oop:hasParameter ( "Nbd021bd5c15b492cbecbac2c71f8bd46" 100 100 100 ) .


Object:Point1_Desc a occ:gp_Pnt ;
    oop:hasParameter ( "N56c43d79a5974e75b1e93e2142057356" 200 200 200) .         

Object:Box2_Desc a occ:BRepPrimAPI_MakeBox ;
    oop:hasMethod Object:Method_Shape ;
    oop:hasParameter ( "N4076d77bdde840689f2713315dbddde1" Object:Point1 100 100 100 ) .
                        